{
  "name": "smart-image-lazyload",
  "version": "0.0.3",
  "description": "React smart image lazy load without width and height",
  "author": "Aryan-mor",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aryan-mor/image-lazyload.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "lint": "concurrently \"eslint src\" \"prettier --check \"./src/**/*.css\"\" ",
    "lint:write": "prettier --write \"./src/**/*.js\"  \"./src/**/*.css\"",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "files": [
    "dist",
    "lib",
    "index.d.ts"
  ],
  "dependencies": {
    "query-string": "^6.13.8"
  },
  "peerDependencies": {
    "clsx": "^1.1.1",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "latest",
    "clsx": "^1.1.1",
    "cross-env": "latest",
    "css-loader": "^2.1.1",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-config-standard": "latest",
    "eslint-config-standard-react": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-standard": "latest",
    "gh-pages": "latest",
    "microbundle-crl": "latest",
    "npm-run-all": "latest",
    "prettier": "latest",
    "prop-types": "^15.7.2",
    "react": "latest",
    "react-dom": "latest",
    "react-scripts": "^4.0.3",
    "style-loader": "^0.23.1"
  },
  "bugs": {
    "url": "https://github.com/Aryan-mor/image-lazyload/issues"
  },
  "homepage": "https://github.com/Aryan-mor/image-lazyload#readme",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "image-lazyload"
  ]
}
